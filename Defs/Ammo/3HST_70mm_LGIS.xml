	<ThingDef ParentName="BuildingBase">
		<defName>Trap_AerialDeployedMine</defName>
		<label>aerial deployed mine</label>
		<description>A tiny anti-personnel mine deployed en masse via LGIS missiles and detonates on touch. Its small size prohibits complicated IFF system to be installed, rendering it impossible to deconstruct it safely, requiring manual detonation via gunfire or EMP blast to defuse. Animals can sense these when calm.</description>
		<thingClass>Building_TrapExplosive</thingClass>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>Things/Building/Aerial_Deployed_Mine</texPath>
			<drawSize>(0.4,0.4)</drawSize>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<altitudeLayer>Filth</altitudeLayer>
		<rotatable>false</rotatable>
		<tickerType>Normal</tickerType>
		<pathCost>0</pathCost>
		<!--
		<neverMultiSelect>true</neverMultiSelect>
		-->
		<selectable>false</selectable>
		<statBases>
			<Mass>0.25</Mass>
			<MaxHitPoints>20</MaxHitPoints>
			<Flammability>1</Flammability>
			<Beauty>-4</Beauty>
			<TrapSpringChance>1.0</TrapSpringChance>
		</statBases>
		<building>
			<isTrap>true</isTrap>
			<trapPeacefulWildAnimalsSpringChanceFactor>0.25</trapPeacefulWildAnimalsSpringChanceFactor>
			<expandHomeArea>false</expandHomeArea>
			<ai_chillDestination>false</ai_chillDestination>
			<deconstructible>false</deconstructible>
			<claimable>false</claimable>
		</building>
		<comps>
			<li Class="CompProperties_Explosive">
				<explosiveDamageType>Bomb</explosiveDamageType>
				<explosiveRadius>1.5</explosiveRadius>
				<damageAmountBase>25</damageAmountBase>
				<startWickOnDamageTaken>
					<li>EMP</li>
					<li>PrometheumFlame</li>
					<li>Flame</li>
				</startWickOnDamageTaken>
				<wickTicks>
					<min>5</min>
					<max>30</max>
				</wickTicks>
			</li>
			<li Class="CombatExtended.CompProperties_ExplosiveCE">
				<damageAmountBase>25</damageAmountBase>
				<explosiveDamageType>Bomb</explosiveDamageType>
				<explosiveRadius>1.2</explosiveRadius>
			</li>
			<li Class="CombatExtended.CompProperties_Fragments">
				<fragments>
					<Fragment_Small>10</Fragment_Small>
				</fragments>
			</li>
			<li Class="ThreeHST.CompProperties_ShowConcept">
				<concepts>
					<li>BDGW_Mines</li>
				</concepts>
			</li>
		</comps>
	</ThingDef>

