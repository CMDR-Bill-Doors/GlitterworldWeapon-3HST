	<ThingDef Abstract="True" Name="PropMedievalBase" ParentName="BuildingBase">
		<fillPercent>0.5</fillPercent>
		<altitudeLayer>Building</altitudeLayer>
		<blockWind>false</blockWind>
		<rotatable>true</rotatable>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<soundImpactDefault>BulletImpact_Wood</soundImpactDefault>
		<drawerType>MapMeshOnly</drawerType>
		<repairEffect>ConstructWood</repairEffect>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<defaultPlacingRot>South</defaultPlacingRot>
		<statBases>
			<Flammability>1</Flammability>
			<Beauty>-20</Beauty>
			<WorkToBuild>500</WorkToBuild>
		</statBases>
		<building>
			<destroySound>BuildingDestroyed_Wood_Medium</destroySound>
