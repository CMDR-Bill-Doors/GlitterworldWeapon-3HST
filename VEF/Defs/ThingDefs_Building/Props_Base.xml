<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef Abstract="True" Name="Prop3HSTBase" ParentName="BuildingBase">
		<fillPercent>0.5</fillPercent>
		<altitudeLayer>Building</altitudeLayer>
		<blockWind>false</blockWind>
		<rotatable>true</rotatable>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<drawerType>MapMeshOnly</drawerType>
		<repairEffect>ConstructMetal</repairEffect>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<defaultPlacingRot>East</defaultPlacingRot>
		<statBases>
			<Flammability>0.2</Flammability>
			<Beauty>-20</Beauty>
			<WorkToBuild>500</WorkToBuild>
		</statBases>
		<building>
			<claimable>false</claimable>
			<destroySound>BuildingDestroyed_Metal_Big</destroySound>
		</building>
		<designationCategory>Furniture</designationCategory>
	</ThingDef>

	<!--Wall light-->
	<ThingDef ParentName="LampBase">
		<defName>abandoned3HSTwallLight</defName>
		<label>abandoned wall light</label>
		<description>An abandoned wall light of 3HST origin, still intact.</description>
		<statBases>
			<Mass>4</Mass>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Props/Abandoned3HSTwallLight/Abandoned3HSTwallLight</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
		</graphicData>
		<rotatable>true</rotatable>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="VanillaFurnitureExpanded.CompProperties_GlowerExtended">
				<spawnGlowerInFacedCell>true</spawnGlowerInFacedCell>
				<colorOptions>
					<li>
						<glowRadius>12</glowRadius>
						<glowColor>(199,252,252,0)</glowColor>
					</li>
				</colorOptions>
			</li>
			<li Class="VanillaFurnitureExpanded.CompProperties_MountableOnWall"/>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>30</basePowerConsumption>
			</li>
		</comps>
		<building>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
			<canPlaceOverWall>true</canPlaceOverWall>
		</building>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<placeWorkers>
			<li>VanillaFurnitureExpanded.PlaceWorker_OnWall</li>
		</placeWorkers>
		<costList>
			<Steel>20</Steel>
		</costList>
		<defaultPlacingRot>South</defaultPlacingRot>
		<designationCategory>Furniture</designationCategory>
		<minifiedDef Inherit="false" IsNull="true"/>
		<thingCategories Inherit="false" IsNull="true"/>
	</ThingDef>

