<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef Abstract="True" Name="Prop3HSTBase" ParentName="BuildingBase">
		<fillPercent>0.5</fillPercent>
		<altitudeLayer>Building</altitudeLayer>
		<blockWind>false</blockWind>
		<rotatable>true</rotatable>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<drawerType>MapMeshOnly</drawerType>
		<repairEffect>ConstructMetal</repairEffect>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<defaultPlacingRot>East</defaultPlacingRot>
		<statBases>
			<Flammability>0.2</Flammability>
			<Beauty>-20</Beauty>
			<WorkToBuild>500</WorkToBuild>
		</statBases>
		<building>
			<claimable>false</claimable>
			<destroySound>BuildingDestroyed_Metal_Big</destroySound>
		</building>
		<designationCategory>Furniture</designationCategory>
	</ThingDef>

	<ThingDef ParentName="DoorBase">
		<defName>Door3HSTabandoned</defName>
		<label>abandoned door</label>
		<description>An abandoned door of 3HST origin.</description>
		<statBases>
			<WorkToBuild>850</WorkToBuild>
			<MaxHitPoints>1600</MaxHitPoints>
			<Flammability>0</Flammability>
		</statBases>
		<stuffCategories Inherit="false"/>
		<useStuffTerrainAffordance>false</useStuffTerrainAffordance>
		<graphicData>
			<texPath>Things/Building/Abandoned3HSTdoor_Mover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>UI/Icons/Abandoned3HSTdoor_MenuIcon</uiIconPath>
		<costStuffCount/>
		<costList>
			<Steel>25</Steel>
		</costList>
	</ThingDef>

	<ThingDef ParentName="Wall">
		<defName>Abandoned3HSTwall</defName>
		<label>abandoned wall</label>
		<description>An impassable wall. Capable of holding up a roof.</description>
		<graphicData>
			<texPath>Things/Building/Abandoned3HSTwall</texPath>
		</graphicData>
		<uiIconPath>UI/Icons/Abandoned3HSTwall_MenuIcon</uiIconPath>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>135</WorkToBuild>
			<Flammability>1.0</Flammability>
			<MeditationFocusStrength>0.22</MeditationFocusStrength>
		</statBases>
		<costStuffCount/>
		<useStuffTerrainAffordance>false</useStuffTerrainAffordance>
		<stuffCategories Inherit="false"/>
		<building>
			<relatedBuildCommands Inherit="false"/>
		</building>
		<costList>
			<Steel>50</Steel>
		</costList>
	</ThingDef>

	<ThingDef ParentName="Wall">
		<defName>Abandoned3HSTwireFence</defName>
		<label>abandoned wire fence</label>
		<description>An impassable wall. Capable of holding up a roof.</description>
		<graphicData>
			<texPath>Things/Building/Abandoned3HSTwireFence</texPath>
			<shadowData>
				<volume>(0.1, 1, 0.1)</volume>
			</shadowData>
		</graphicData>
		<uiIconPath>UI/Icons/Abandoned3HSTwireFence_MenuIcon</uiIconPath>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>135</WorkToBuild>
			<Flammability>1.0</Flammability>
			<MeditationFocusStrength>0.22</MeditationFocusStrength>
		</statBases>
		<costStuffCount/>
		<useStuffTerrainAffordance>false</useStuffTerrainAffordance>
		<stuffCategories Inherit="false"/>
		<coversFloor>false</coversFloor>
		<holdsRoof>false</holdsRoof>
		<fillPercent>0</fillPercent>
		<blockWind>false</blockWind>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>
		<building>
			<relatedBuildCommands Inherit="false"/>
		</building>
		<costList>
			<Steel>50</Steel>
		</costList>
	</ThingDef>

	<ThingDef Abstract="True" ParentName="LampBase" Name="3HSTwallLightBase">
		<graphicData>
			<texPath>Things/Building/Props/Abandoned3HSTwallLight/Abandoned3HSTwallLight</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
		</graphicData>
		<rotatable>true</rotatable>
		<comps>
			<li Class="VanillaFurnitureExpanded.CompProperties_MountableOnWall"/>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>30</basePowerConsumption>
			</li>
		</comps>
		<building>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
			<canPlaceOverWall>true</canPlaceOverWall>
		</building>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<placeWorkers>
			<li>VanillaFurnitureExpanded.PlaceWorker_OnWall</li>
		</placeWorkers>
		<defaultPlacingRot>South</defaultPlacingRot>
		<costList>
			<Steel>25</Steel>
		</costList>
		<minifiedDef Inherit="false"/>
		<thingCategories Inherit="false"/>
		<designationCategory>Furniture</designationCategory>
	</ThingDef>

	<ThingDef ParentName="3HSTwallLightBase">
		<defName>abandoned3HSTwallLight</defName>
		<label>abandoned wall light</label>
		<description>An abandoned wall light of 3HST origin, still intact.</description>
		<comps>
			<li Class="VanillaFurnitureExpanded.CompProperties_GlowerExtended">
				<spawnGlowerInFacedCell>true</spawnGlowerInFacedCell>
				<colorOptions>
					<li>
						<glowRadius>12</glowRadius>
						<glowColor>(199,252,252,0)</glowColor>
					</li>
				</colorOptions>
			</li>
		</comps>
	</ThingDef>

	<ThingDef ParentName="3HSTwallLightBase">
		<defName>abandoned3HSTwallLightDimmed</defName>
		<label>faulty abandoned wall light</label>
		<description>An abandoned wall light of 3HST origin, a faulty circuit resulted in reduced brightness.</description>
		<comps>
			<li Class="VanillaFurnitureExpanded.CompProperties_GlowerExtended">
				<spawnGlowerInFacedCell>true</spawnGlowerInFacedCell>
				<colorOptions>
					<li>
						<glowRadius>12</glowRadius>
						<glowColor>(100,126,126,0)</glowColor>
					</li>
				</colorOptions>
			</li>
		</comps>
	</ThingDef>


	<ThingDef ParentName="Prop3HSTBase">
